                <Label Name="txtM" Content="M" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="20" FontFamily="Franklin Gothic Heavy" Foreground="#B6D7A8"/>
            </Border>
            <Border Background="#607E54" BorderThickness="0" CornerRadius="10" Height="40" Width="Auto" Margin="1">
                <Label Name="txtBackSpace" Content="Back" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="20" FontFamily="Franklin Gothic Heavy" Foreground="#B6D7A8"/>
