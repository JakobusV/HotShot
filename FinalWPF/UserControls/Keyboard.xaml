            </Border>
        </StackPanel>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Border Name="brdA" Background="#607E54" BorderThickness="0" CornerRadius="10" Height="40" Width="40" Margin="1">
                <Label Name="txtA" Content="A" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="20" FontFamily="Franklin Gothic Heavy" Foreground="#B6D7A8"/>
